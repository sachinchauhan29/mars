<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta http-equiv="Content-Language" content="en">
    <title> Admin-MARS</title>
    <meta name="viewport"
        content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no, shrink-to-fit=no" />
    <link rel="icon" type="image/x-icon" href="../images/ficon (1).png">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/apexcharts@3.26.0/dist/apexcharts.min.css">
    <meta name="msapplication-tap-highlight" content="no">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.5.0/css/all.css"
        integrity="sha384-B4dIYHKNBt8Bc12p+WXckhzcICo0wtJAoU8YZTY5qE0Id1GSseTk6S+L3BlXeVIU" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="../css/style.css">
    <!-- jQuery -->
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>

<body id="element">
    <div class="app-container app-theme-white body-tabs-shadow fixed-sidebar fixed-header">
        <div class="app-header header-shadow">
            <div class="app-header__logo text-center">
                <div class="logo-src">
                </div>
                <div class="header__pane ml-auto">
                    <div>
                        <button type="button" class="hamburger close-sidebar-btn hamburger--elastic"
                            data-class="closed-sidebar">
                            <span class="hamburger-box">
                                <span class="hamburger-inner"></span>
                            </span>
                        </button>
                    </div>
                </div>
            </div>
            <div class="app-header__mobile-menu">
                <div>
                    <button type="button" class="hamburger hamburger--elastic mobile-toggle-nav">
                        <span class="hamburger-box">
                            <span class="hamburger-inner"></span>
                        </span>
                    </button>
                </div>
            </div>
            <div class="app-header__menu">
                <span>
                    <button type="button"
                        class="btn-icon btn-icon-only btn btn-primary custom-btnbtn-sm mobile-toggle-header-nav">
                        <span class="btn-icon-wrapper">
                            <i class="fa fa-ellipsis-v fa-w-6"></i>
                        </span>
                    </button>
                </span>
            </div>
            <div class="app-header__content">
                <div class="app-header-left">
                    <div class="search-wrapper">
                        <!-- <div class="input-holder">
                            <input type="text" class="search-input" placeholder="Type to search">
                            <button class="search-icon"><span></span></button>
                        </div> -->
                        <button class="close"></button>
                    </div>

                </div>
                <div class="app-header-right">
                    <div class="header-btn-lg pr-0">
                        <div class="widget-content p-0">
                            <div class="widget-content-wrapper">

                                <div class="widget-content-left">
                                    <div class="btn-group">
                                        <a data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"
                                            class="p-0 btn">
                                            <div class="badge badge-danger custom-button mr-1 ml-0 p-2">
                                                <b>
                                                    <%= notification.length%>
                                                </b> <i class="fa fa-bell"></i>
                                                <i class="fa fa-angle-down ml-2 opacity-8"></i>
                                            </div>
                                        </a>
                                        <div tabindex="-1" role="menu" aria-hidden="true"
                                            class="dropdown-menu dropdown-menu-right">


                                            <p class="dropdown-item">Notifications</p>
                                            <div tabindex="-1" class="dropdown-divider"></div>
                                            <% notification.forEach((ele)=> { %>
                                                <% if (ele.kyc_type==='Edit-KYC-Request' ) { %>
                                                    <a href="/kyc/kyc-notification?notify_id= <%= ele.id %>">
                                                        <p class="dropdown-item">

                                                            <span class="list-awsm">
                                                                <%= ele.id %>
                                                            </span> <span class="list-awsm">
                                                                <%= ele.message %>
                                                            </span>
                                                            <br>
                                                            <div tabindex="-1" class="dropdown-divider"></div>

                                                        </p>
                                                    </a>
                                                    <% } else { %>
                                                        <a
                                                            href="/edit-kyc-data/edit-kyc-notification?notify_id= <%= ele.id %>">
                                                            <p class="dropdown-item">

                                                                <span class="list-awsm">
                                                                    <%= ele.id %>
                                                                </span> <span class="list-awsm">
                                                                    <%= ele.message %>
                                                                </span>
                                                                <br>
                                                                <div tabindex="-1" class="dropdown-divider"></div>

                                                            </p>
                                                        </a>
                                                        <% } %>
                                                            <% }); %>

                                                                <p class="dropdown-item text-center">
                                                                    <a class="view" href="./notification">VIEW</a>
                                                                </p>
                                        </div>

                                    </div>
                                </div>
                                <div class="widget-content-left  ml-3 header-user-info">

                                </div>
                                <div class="widget-content-right header-user-info ml-3">

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="app-main">
            <div class="app-sidebar sidebar-shadow">
                <div class="app-header__logo">
                    <div class="logo-src"></div>
                    <div class="header__pane ml-auto">
                        <div>
                            <button type="button" class="hamburger close-sidebar-btn hamburger--elastic"
                                data-class="closed-sidebar">
                                <span class="hamburger-box">
                                    <span class="hamburger-inner"></span>
                                </span>
                            </button>
                        </div>
                    </div>
                </div>
                <div class="app-header__mobile-menu">
                    <div>
                        <button type="button" class="hamburger hamburger--elastic mobile-toggle-nav">
                            <span class="hamburger-box">
                                <span class="hamburger-inner"></span>
                            </span>
                        </button>
                    </div>
                </div>
                <div class="app-header__menu">
                    <span>
                        <button type="button"
                            class="btn-icon btn-icon-only btn btn-primary custom-btnbtn-sm mobile-toggle-header-nav">
                            <span class="btn-icon-wrapper">
                                <i class="fa fa-ellipsis-v fa-w-6"></i>
                            </span>
                        </button>
                    </span>
                </div>
                <div class="scrollbar-sidebar">
                    <div class="app-sidebar__inner">
                        <ul class="vertical-nav-menu">

                            <% if(user.role==='admin' ){%>
                                <li>
                                    <a href="/dashboard" class="mm-activew">
                                        <i class="metismenu-icon fa fa-home"></i>
                                        Dashboard
                                    </a>
                                </li>
                                <%} else{%>

                                    <%} %>

                                        <li>
                                            <a href="/kyc">
                                                <i class="metismenu-icon fas fa-sort-amount-up"></i>
                                                Program Performance
                                            </a>
                                        </li>

                                        <li>
                                            <a href="/upload-payout">
                                                <i class="metismenu-icon fa fa-cloud-upload "></i>
                                                Upload Payout
                                            </a>
                                        </li>

                                        <% if(user.role==='admin' ){%>
                                            <li>
                                                <a href="/data-whitelisting">
                                                    <i class="metismenu-icon fas fa-clone "></i>
                                                    DSR Data Whitelisting
                                                </a>
                                            </li>
                                            <li>
                                                <a href="/retailerwhitelisting">
                                                    <i class="metismenu-icon fas fa-clone "></i>
                                                    Retailer Data whitelisting
                                                </a>
                                            </li>
                                            <%} else{%>
                                                <li>
                                                    <a>
                                                        <i class="metismenu-icon fas fa-clone "></i>
                                                        DSR Data Whitelisting
                                                    </a>
                                                </li>
                                                <li>
                                                    <a>
                                                        <i class="metismenu-icon fas fa-clone "></i>
                                                        Retailer Data whitelisting
                                                    </a>
                                                </li>
                                                <%} %>
                                                    <!-- <li>
                                <a href="/block-ase-user-report">
                                    <i class="metismenu-icon fas fa-clone "></i>
                                    Blocked ASE User Report
                                </a>
                            </li> -->
                                                    <!-- <li>
                                            <a href="/report">
                                                <i class="metismenu-icon fa fa fa-file"></i>
                                                Report
                                            </a>
                                        </li> -->

                                                    <% if( user.role==='admin' ){%>
                                                        <li>
                                                            <a href="/users">
                                                                <i class="metismenu-icon fas fa-clone "></i>
                                                                Users
                                                            </a>
                                                        </li>
                                                        <%}%>

                                                            <li>
                                                                <a href="./account/user-logout">
                                                                    <i class="metismenu-icon fa fa-sign-out"></i>
                                                                    Logout
                                                                </a>
                                                            </li>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="app-main__outer">
                <div class="app-main__inner">